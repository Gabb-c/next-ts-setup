{
  "name": "next-ts-setup",
  "version": "0.1.0",
  "private": false,
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "description": "A React.js setup using Next.js",
  "repository": {
    "url": "https://github.com/Gabb-c/next-ts-setup"
  },
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "test": "jest",
    "lint": "eslint src --ext js,ts,tsx --ignore-path .prettierignore",
    "lint:fix": "eslint src --ext js,ts,tsx --fix --ignore-path .prettierignore",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "tsc": "tsc --skipLibCheck",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "next": "^10.2.3",
    "next-images": "^1.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^7.0.4",
    "sass": "^1.32.13",
    "styled-components": "^5.3.0",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@next/bundle-analyzer": "^10.2.0",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.3.0-alpha.19",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/styled-components": "^5.1.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "css-loader": "^5.2.4",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "install-peers": "^1.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "next-unused": "^0.0.6",
    "prettier": "^2.2.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src/pages"
    ],
    "debug": true,
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  }
}
