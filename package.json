{
  "name": "next-ts-setup",
  "version": "0.1.0",
  "private": false,
  "description": "A React.js setup using Next.js",
  "repository": {
    "url": "https://github.com/Gabb-c/next-ts-setup"
  },
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "eslint src --ext js,ts,tsx --ignore-path .prettierignore",
    "lint:fix": "eslint src --ext js,ts,tsx --fix --ignore-path .prettierignore",
    "lint:ci": "yarn lint:fix && yarn format && yarn sort",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "sort": "sort-package-json package.json",
    "tsc": "tsc --skipLibCheck",
    "test:watch": "jest --watch --verbose --colors --expand --maxWorkers=25%",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated",
    "test:ci": "jest --ci --runInBand --bail",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prepare": "is-ci || husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "next": "^12.0.2",
    "next-images": "^1.8.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.1.0",
    "sass": "^1.43.4",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@next/bundle-analyzer": "^12.0.2",
    "@next/eslint-plugin-next": "^12.0.2",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.1.0",
    "eslint-config-next": "^12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-unicorn": "^38.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "next-unused": "^0.0.6",
    "prettier": "^2.4.1",
    "sass-loader": "^12.3.0",
    "sort-package-json": "^1.52.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src/pages"
    ],
    "debug": true,
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  }
}
