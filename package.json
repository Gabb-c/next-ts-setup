{
  "name": "next-ts-setup",
  "version": "0.1.0",
  "private": false,
  "description": "A React.js setup using Next.js",
  "repository": {
    "url": "https://github.com/Gabb-c/next-ts-setup"
  },
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "eslint src --ext js,ts,tsx --ignore-path .prettierignore && yarn stylelint src/**/*.{scss,css}",
    "lint:fix": "eslint src --ext js,ts,tsx --fix --ignore-path .prettierignore && yarn stylelint src/**/*.{scss,css} --fix",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "sort": "sort-package-json package.json",
    "tsc": "tsc --skipLibCheck",
    "test:watch": "jest --watch --verbose --colors --expand --maxWorkers=25%",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated",
    "test:ci": "jest --ci --runInBand --bail",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "next": "^12.0.4",
    "next-images": "^1.8.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.1.0",
    "sass": "^1.44.0",
    "swr": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@next/bundle-analyzer": "^12.0.4",
    "@next/eslint-plugin-next": "^12.0.4",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.4.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "next-unused": "^0.0.6",
    "prettier": "^2.5.0",
    "sass-loader": "^12.3.0",
    "sort-package-json": "^1.53.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src/pages"
    ],
    "debug": true,
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  }
}
