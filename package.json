{
  "name": "next-ts-setup",
  "version": "0.1.0",
  "private": false,
  "description": "A React.js setup using Next.js",
  "repository": {
    "url": "https://github.com/Gabb-c/next-ts-setup"
  },
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "eslint src --ext js,ts,tsx --ignore-path .prettierignore && yarn stylelint src/**/*.{scss,css}",
    "lint:fix": "eslint src --ext js,ts,tsx --fix --ignore-path .prettierignore && yarn stylelint src/**/*.{scss,css} --fix",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "sort": "sort-package-json package.json",
    "tsc": "tsc --skipLibCheck",
    "test:watch": "jest --watch --verbose --colors --expand --maxWorkers=25%",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated",
    "test:ci": "jest --ci --runInBand --bail",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "next": "^12.1.6",
    "next-images": "^1.8.4",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-toastify": "^9.0.1",
    "sass": "^1.52.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@next/eslint-plugin-next": "^12.1.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.1",
    "next-unused": "^0.0.6",
    "prettier": "^2.6.2",
    "sass-loader": "^13.0.0",
    "sort-package-json": "^1.57.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src/pages"
    ],
    "debug": true,
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  }
}
