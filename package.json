{
  "name": "next-ts-setup",
  "version": "0.1.0",
  "private": false,
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "description": "A React.js setup using Next.js",
  "repository": {
    "url": "https://github.com/Gabb-c/next-ts-setup"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint src --ext js,ts,tsx --ignore-path .prettierignore",
    "lint:fix": "eslint src --ext js,ts,tsx --fix --ignore-path .prettierignore",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "tsc": "tsc --skipLibCheck",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
