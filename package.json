    "react-toastify": "^7.0.3",
    "styled-components": "^5.2.3",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@next/bundle-analyzer": "^10.1.3",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
